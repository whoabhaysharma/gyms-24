          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "if (jsonData.data) {",
                  "    if (jsonData.data.subscriptionId) pm.environment.set(\"subscriptionId\", jsonData.data.subscriptionId);",
                  "    if (jsonData.data.paymentId) pm.environment.set(\"paymentId\", jsonData.data.paymentId);",
                  "    if (jsonData.data.orderId) pm.environment.set(\"razorpayOrderId\", jsonData.data.orderId);",
                  "}"
                ],
